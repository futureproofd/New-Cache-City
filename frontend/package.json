{
  "name": "newcachecity-frontend",
  "version": "1.0.0",
  "description": "A Geocaching Web Application built with React",
  "main": "index.js",
  "//": "Note the scripts below reference two .env configs",
  "scripts": {
    "prod": "webpack-dev-server --env.ENVIRONMENT=production --config ./webpack.config.js --mode production",
    "_devOld": "webpack-dev-server --env.ENVIRONMENT=development --config ./webpack.config.js --mode development",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config ./webpack.server.js --mode development --env.ENVIRONMENT=development --watch",
    "dev:build-client": "webpack --config ./webpack.client.js --mode development --env.ENVIRONMENT=development --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Marcus Plienegger",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "axios": "^0.19.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "google-map-react": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-google-recaptcha": "^2.0.1",
    "react-redux": "^7.1.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.4.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^2.1.0",
    "styled-components": "^4.3.2",
    "webpack": "^4.40.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
