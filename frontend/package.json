{
  "name": "newcachecity-frontend",
  "version": "1.0.0",
  "description": "A Geocaching Web Application built with React",
  "main": "index.js",
  "//": "Note the scripts below reference two .env configs",
  "scripts": {
    "prod": "webpack-dev-server --env.ENVIRONMENT=production --config ./webpack.config.js --mode production",
    "dev": "webpack-dev-server --env.ENVIRONMENT=development --config ./webpack.config.js --mode development",
    "test": "SET NODE_ENV=test&& jest --watch"
  },
  "author": "Marcus Plienegger",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "axios": "^0.19.0",
    "downshift": "^3.4.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "google-map-react": "^1.1.5",
    "jest": "^24.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-google-recaptcha": "^2.0.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.4.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    }
  }
}
